int sumArray(int arr[]) {
	int len;
	int i;
	int sum;

	len = length(arr);
	i=0;
	sum = 0;
	while (i < len) {
		sum = sum + arr[i];
	}

	return sum;
}

void main(void) {
	int somearr[1];

	somearr[0] = 10;
	somearr[1] = argc;
	somearr[argc - argc + 1] = 2+2;

	printf( sumArray(somearr) );
